{
	"name": "@ovh-ux/manager-catalog-app",
	"version": "0.0.1",
	"private": true,
	"license": "BSD-3-Clause",
	"author": "OVH SAS",
	"type": "module",
	"scripts": {
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "vite dev",
		"format": "prettier --plugin-search-dir . --write .",
		"lint": "prettier --plugin-search-dir . --check .",
		"preview": "vite preview",
		"start": "lerna exec --stream --scope='@ovh-ux/manager-catalog-app' --include-dependencies -- npm run build --if-present",
		"start:dev": "lerna exec --stream --scope='@ovh-ux/manager-catalog-app' --include-dependencies -- npm run dev --if-present",
		"test": "playwright test",
		"test:unit": "vitest"
	},
	"dependencies": {
		"@ovh-ux/manager-config": "^6.5.2",
		"@ovh-ux/request-tagger": "^0.1.1",
		"@ovh-ux/shell": "^1.8.2"
	},
	"devDependencies": {
		"@ovh-ux/manager-models": "^1.14.3",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/adapter-static": "^2.0.1",
		"@sveltejs/kit": "^1.0.0",
		"autoprefixer": "^10.4.13",
		"moment": "^2.29.4",
		"postcss": "^8.4.21",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"sveltekit-i18n": "^2.2.2",
		"tailwindcss": "^3.2.4",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0",
		"vitest": "^0.25.3"
	},
	"regions": [
		"CA",
		"EU",
		"US"
	]
}
